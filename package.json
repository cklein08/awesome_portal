{
  "name": "koassets",
  "private": true,
  "version": "1.0.0",
  "description": "KO Assets Portal",
  "scripts": {
    "install-all": "npm install && cd koassets-react && npm install && cd ../cloudflare && npm install",
    "dev": "./local.sh",
    "lint:js": "eslint .",
    "lint:css": "stylelint \"blocks/**/*.css\" \"styles/*.css\"",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js -- --fix && npm run lint:css -- --fix",
    "prepare": "husky"
  },
  "devDependencies": {
    "@adobe/aem-cli": "16.10.46",
    "@babel/eslint-parser": "7.27.1",
    "eslint": "8.57.1",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-plugin-import": "2.31.0",
    "husky": "9.1.7",
    "stylelint": "16.19.1",
    "stylelint-config-standard": "38.0.0"
  }
}
